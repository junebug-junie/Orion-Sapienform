# Service-local overrides. Root .env already has PROJECT, NODE_NAME, STORAGE_ROOT, ORION_BUS_URL.
ORION_BUS_ENFORCE_CATALOG=true
SERVICE_NAME=gpu-cluster-power
SERVICE_VERSION=0.1.0

# Enable bus for this service
#ORION_BUS_URL=redis://100.92.216.81:6379/0
ORION_BUS_ENABLED=true

# PSU board base + paths (replace with real ones from the web UI network tab)
PSU_BASE_URL=http://192.168.1.100

# paths
PSU_ON_PATH=/power
PSU_OFF_PATH=/power
PSU_CYCLE_PATH=/power

PORT=8050

API_TOKEN=super-secret-token-change-me
