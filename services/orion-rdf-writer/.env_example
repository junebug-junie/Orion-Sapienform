janus@janus:/mnt/services/Orion-Sapienform/services/orion-rdf-writer$ cat .env
# RDF + GraphDB
GRAPHDB_URL=http://graphdb:7200
GRAPHDB_REPO=collapse
STATEMENTS_URL=http://graphdb:7200/repositories/collapse/statements


GRAPHDB_USER=user
GRAPHDB_PASS=pass

# Core bus (shared)
ORION_BUS_URL=redis://orion-redis:6379/0
ORION_BUS_CHANNEL=orion:events:tagged

# Bus channels (multi-domain)
CHANNEL_EVENTS_TAGGED=orion:events:tagged
CHANNEL_RDF_ENQUEUE=orion:rdf:enqueue
CHANNEL_CORE_EVENTS=orion:core:events
CHANNEL_RDF_CONFIRM=orion:rdf:confirm
CHANNEL_RDF_ERROR=orion:rdf:error

# Service configuration
SERVICE_NAME=orion-rdf-writer
LOG_LEVEL=DEBUG
BATCH_SIZE=1
RETRY_LIMIT=3
RETRY_INTERVAL=2
