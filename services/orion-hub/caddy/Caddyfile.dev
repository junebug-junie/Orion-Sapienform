http://hub.conjourney.net {
    encode gzip zstd

    header {
        Access-Control-Allow-Origin *
        Access-Control-Allow-Methods "GET, POST, OPTIONS"
        Access-Control-Allow-Headers "Content-Type, Authorization"
    }

    # If hub UI is on the host:
    reverse_proxy 127.0.0.1:8080
    # If itâ€™s another container on the same docker network, use its service name instead:
    # reverse_proxy orion-atlas-hub-ui:8080
}
