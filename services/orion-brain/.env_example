cat .env
# --- Service identity ---
SERVICE_NAME=orion-brain
SERVICE_VERSION=0.1.0

# --- Redis / Orion Bus ---
REDIS_URL=redis://orion-redis:6379/0

# --- Event intake (streaming in) ---
EVENTS_ENABLE=true
EVENTS_STREAM=orion:evt:gateway

# --- Bus output (publishing out) ---
BUS_OUT_ENABLE=true
BUS_OUT_STREAM=orion:bus:out
